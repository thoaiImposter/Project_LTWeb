
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">
    <display-name>MovieBookingApp</display-name>

    <servlet>
        <servlet-name>MovieBookingServlet</servlet-name>
        <servlet-class>viduServlet.MovieBookingServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MovieBookingServlet</servlet-name>
        <url-pattern>/movie</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    
    
</web-app>
